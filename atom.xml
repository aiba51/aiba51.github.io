  <entry>
    <title>今天考完试了</title>
    <link href="https://aixz.xyz/%E6%97%A5%E5%B8%B8%E8%AE%B0%E5%BD%95/%E4%BB%8A%E5%A4%A9%E8%80%83%E5%AE%8C%E8%AF%95%E4%BA%86/"/>
